{
  "name": "protobuilder",
  "version": "0.0.48",
  "description": "Rapid Prototyping & Development",
  "main": "dist/index.js",
  "scripts": {
    "start": "npm run watch",
    "dev": "webpack-dev-server --open --config ./dev/config/webpack.config.js",
    "watch": "onchange 'src/**/*.js' 'src/**/*.css' -- npm run transpile",
    "transpile": "babel src -d dist --copy-files",
    "dist": "npm run transpile && git add . && git commit -m 'update' && git push && npm version patch && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vargusf/protobuilder"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://github.com/vargusf/protobuilder#readme",
  "dependencies": {
    "autoprefixer": "^9.4.7",
    "babel-cli": "^6.26.0",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "css-loader": "^2.1.0",
    "cssnano": "^4.1.9",
    "file-loader": "^3.0.1",
    "onchange": "^5.2.0",
    "postcss": "^7.0.14",
    "postcss-loader": "^3.0.0",
    "postcss-mixins": "^6.2.1",
    "postcss-nested": "^4.1.1",
    "postcss-preset-env": "^6.5.0",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-router-dom": "^4.3.1",
    "style-loader": "^0.23.1",
    "webpack": "4.16.1",
    "webpack-cli": "3.0.8"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-class-properties"
    ]
  },
  "devDependencies": {
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "webpack-dev-server": "^3.1.4"
  }
}
