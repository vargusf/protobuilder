{
  "name": "protobuilder",
  "version": "0.0.45",
  "description": "Rapid Prototyping & Development",
  "main": "dist/index.js",
  "scripts": {
    "start": "webpack-dev-server --open --config ./dev/config.js",
    "watch": "onchange 'src/**/*.js' -- npm run transpile",
    "transpile": "babel src -d dist --copy-files",
    "dist": "npm run transpile && git add . && git commit -m 'update' && git push && npm version patch && npm publish"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/vargusf/protobuilder"
  },
  "author": "",
  "license": "ISC",
  "homepage": "https://github.com/vargusf/protobuilder#readme",
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "6.26.3",
    "babel-loader": "7.1.5",
    "babel-plugin-transform-class-properties": "6.24.1",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "6.24.1",
    "babel-preset-stage-0": "6.24.1",
    "babel-register": "6.26.0",
    "onchange": "^5.2.0",
    "react": "16.4.2",
    "react-dom": "16.4.2",
    "react-router-dom": "^4.3.1",
    "webpack": "4.16.1",
    "webpack-cli": "3.0.8"
  },
  "babel": {
    "presets": [
      "env",
      "react"
    ],
    "plugins": [
      "transform-object-rest-spread",
      "transform-class-properties"
    ]
  },
  "devDependencies": {
    "css-loader": "^2.1.0",
    "node-sass": "^4.11.0",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "webpack-dev-server": "^3.1.4"
  }
}
